lw 1,0,a
lw 2,0,b
lw 3,0,c
nand 3,3,3 #gambiarra para realiar subtração, transforma o valor de c em negativo
addi 3,3,1
add 4,1,2
add 5,3,4     #gambiarra para realizar subtração
lui 6,511    #gambiarra eliminar os 4 bits iniciais
addi 6,6,63   #gambiarra paraconseguir comparar numero menor ou maior que zero
nand 7,5,6 #teste
beq 7,6,17
addi 1,1,1  #inicio do while
addi 2,2,2
addi 3,3,-1
beq 0,0,teste #zera o registrador

sw 1,0,a
sw 2,0,b
sw 3,0,c
halt

a: .fill 10
b: .fill 12
c: .fill 30
